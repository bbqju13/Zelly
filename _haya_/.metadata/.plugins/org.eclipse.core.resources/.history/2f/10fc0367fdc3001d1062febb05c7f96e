	//생성자에서 학생 리스트 초기화
	public StudentManager() {
		this.studentList = new ArrayList<>();
	}

	public void printStudent() {
		// TODO Auto-generated method stub
		for(int i=0; i<studentList.size(); i++) {
			System.out.println(studentList.get(i));
		}
		
	}

